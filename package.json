{
	"name": "fitpal",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"pod-install": "cd ios && pod install && cd ..",
		"preandroid": "yarn",
		"preios": "yarn && yarn run pod-install",
		"android": "react-native run-android",
		"ios": "react-native run-ios --simulator=\"iPhone 8\"",
		"metro": "react-native start --reset-cache",
		"test": "jest",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx"
	},
	"dependencies": {
		"@react-native-community/async-storage": "^1.9.0",
		"@react-native-community/masked-view": "^0.1.6",
		"axios": "^0.19.2",
		"axios-mock-adapter": "^1.18.1",
		"big.js": "^5.2.2",
		"i18next": "^19.3.3",
		"lodash": "^4.17.13",
		"moment": "^2.24.0",
		"react": "16.11.0",
		"react-i18next": "^11.3.4",
		"react-native": "0.62.2",
		"react-native-calendar-strip": "^2.0.3",
		"react-native-calendars": "^1.289.0",
		"react-native-datepicker": "^1.7.2",
		"react-native-draggable-flatlist": "^2.2.0",
		"react-native-elements": "^2.0.0",
		"react-native-gesture-handler": "^1.5.3",
		"react-native-modal": "^11.5.6",
		"react-native-picker-select": "^6.5.0",
		"react-native-reanimated": "^1.7.0",
		"react-native-safe-area-context": "^0.6.4",
		"react-native-screens": "^2.0.0-alpha.32",
		"react-native-svg": "^12.1.0",
		"react-native-swipe-gestures": "^1.0.5",
		"react-native-swipe-list-view": "^2.1.3",
		"react-native-vector-icons": "^6.6.0",
		"react-navigation": "^4.0.10",
		"react-navigation-stack": "^2.0.16",
		"react-redux": "^7.1.3",
		"redux": "^4.0.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"uuid": "^3.4.0",
		"victory-native": "^34.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.2",
		"@babel/runtime": "^7.6.2",
		"@react-native-community/eslint-config": "^1.1.0",
		"@types/axios": "^0.14.0",
		"@types/big.js": "^4.0.5",
		"@types/jest": "^24.0.24",
		"@types/lodash": "^4.14.155",
		"@types/node": "^13.13.0",
		"@types/react-native": "^0.60.25",
		"@types/react-native-calendars": "^1.20.7",
		"@types/react-native-datepicker": "^1.7.0",
		"@types/react-native-vector-icons": "^6.4.5",
		"@types/react-redux": "^7.1.7",
		"@types/react-test-renderer": "16.9.1",
		"@types/uuid": "^3.4.7",
		"@typescript-eslint/eslint-plugin": "^2.12.0",
		"@typescript-eslint/parser": "^2.12.0",
		"babel-jest": "^24.9.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"cz-conventional-changelog": "^3.1.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react-hooks": "^4.0.0",
		"husky": "^4.2.1",
		"jest": "^25.3.0",
		"lint-staged": "^10.0.3",
		"metro-react-native-babel-preset": "^0.58.0",
		"node": "^13.12.0",
		"prettier": "^1.19.1",
		"react-test-renderer": "^16.13.1",
		"redux-devtools-extension": "^2.13.8",
		"typescript": "^3.7.3"
	},
	"jest": {
		"preset": "react-native",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{json,css,html}": [
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write",
			"jest --bail --findRelatedTests"
		],
		"src/i18n/messages/*.json": [
			"node scripts/compare-json-file-keys.js",
			"node scripts/sort-json-file.js",
			"prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
